{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'HomeView',\n  data() {\n    return {\n      showSignupForm: false,\n      // 로그인 폼과 회원가입 폼을 전환하기 위한 변수\n      username: '',\n      email: '',\n      password: '',\n      recommenderId: '' // 추천인 ID 필드 추가\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:3000/login', {\n          email: this.email,\n          password: this.password\n        });\n        localStorage.setItem('token', response.data.token); // 로그인 성공 시 토큰 저장\n\n        // 리디렉션 추가\n        this.$router.push({\n          name: 'MainPage'\n        });\n        alert('로그인 성공');\n      } catch (error) {\n        console.error(error);\n        alert('로그인 실패');\n      }\n    },\n    async signup() {\n      try {\n        // 회원가입 시 추천인 ID를 전달하는 부분 추가\n        const data = {\n          username: this.username,\n          email: this.email,\n          password: this.password\n        };\n        if (this.recommenderId) {\n          data.recommender_id = this.recommenderId; // 추천인 ID가 있을 경우 추가\n        }\n        await axios.post('http://localhost:3000/signup', data);\n        alert('회원가입 성공');\n        this.showSignupForm = false; // 회원가입 후 로그인 폼으로 돌아가기\n      } catch (error) {\n        console.error(error);\n        alert('회원가입 실패');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","showSignupForm","username","email","password","recommenderId","methods","login","response","post","localStorage","setItem","token","$router","push","alert","error","console","signup","recommender_id"],"sources":["C:\\Users\\Tm\\my-new-front\\src\\views\\HomeView.vue"],"sourcesContent":["<!-- HomeView.vue -->\n\n<template>\n  <div>\n    <h1>홈 화면</h1>\n    <div>\n      <button @click=\"showSignupForm = !showSignupForm\">\n        {{ showSignupForm ? '로그인 화면 보기' : '회원가입 화면 보기' }}\n      </button>\n    </div>\n\n    <!-- 로그인 폼 -->\n    <div v-if=\"!showSignupForm\">\n      <h2>로그인</h2>\n      <form @submit.prevent=\"login\">\n        <input v-model=\"email\" placeholder=\"이메일\" required />\n        <input v-model=\"password\" type=\"password\" placeholder=\"비밀번호\" required />\n        <button type=\"submit\">로그인</button>\n      </form>\n    </div>\n\n    <!-- 회원가입 폼 -->\n    <div v-if=\"showSignupForm\">\n      <h2>회원가입</h2>\n      <form @submit.prevent=\"signup\">\n        <input v-model=\"username\" placeholder=\"사용자명\" required />\n        <input v-model=\"email\" placeholder=\"이메일\" required />\n        <input v-model=\"password\" type=\"password\" placeholder=\"비밀번호\" required />\n        <!-- 추천인 ID 입력 필드 추가 -->\n        <input v-model=\"recommenderId\" placeholder=\"추천인 ID (선택)\" />\n        <button type=\"submit\">회원가입</button>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'HomeView',\n  data() {\n    return {\n      showSignupForm: false,  // 로그인 폼과 회원가입 폼을 전환하기 위한 변수\n      username: '',\n      email: '',\n      password: '',\n      recommenderId: '',  // 추천인 ID 필드 추가\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('http://localhost:3000/login', {\n          email: this.email,\n          password: this.password,\n        });\n        localStorage.setItem('token', response.data.token);  // 로그인 성공 시 토큰 저장\n\n        // 리디렉션 추가\n        this.$router.push({ name: 'MainPage' });\n\n        alert('로그인 성공');\n      } catch (error) {\n        console.error(error);\n        alert('로그인 실패');\n      }\n    },\n    async signup() {\n      try {\n        // 회원가입 시 추천인 ID를 전달하는 부분 추가\n        const data = {\n          username: this.username,\n          email: this.email,\n          password: this.password,\n        };\n        if (this.recommenderId) {\n          data.recommender_id = this.recommenderId;  // 추천인 ID가 있을 경우 추가\n        }\n\n        await axios.post('http://localhost:3000/signup', data);\n        alert('회원가입 성공');\n        this.showSignupForm = false;  // 회원가입 후 로그인 폼으로 돌아가기\n      } catch (error) {\n        console.error(error);\n        alert('회원가입 실패');\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\nbutton {\n  padding: 10px;\n  background-color: #4CAF50;\n  color: white;\n  border: none;\n  cursor: pointer;\n}\n\nbutton:hover {\n  background-color: #45a049;\n}\n\nform {\n  margin-top: 20px;\n}\n\ninput {\n  margin: 10px 0;\n  padding: 8px;\n  width: 100%;\n  max-width: 300px;\n}\n\nh2 {\n  margin-bottom: 15px;\n}\n</style>\n"],"mappings":";AAqCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,KAAK;MAAG;MACxBC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,aAAa,EAAE,EAAE,CAAG;IACtB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,IAAI,CAAC,6BAA6B,EAAE;UAC/DN,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACFM,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,QAAQ,CAACR,IAAI,CAACY,KAAK,CAAC,EAAG;;QAErD;QACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;UAAEf,IAAI,EAAE;QAAW,CAAC,CAAC;QAEvCgB,KAAK,CAAC,QAAQ,CAAC;MACjB,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;QACpBD,KAAK,CAAC,QAAQ,CAAC;MACjB;IACF,CAAC;IACD,MAAMG,MAAMA,CAAA,EAAG;MACb,IAAI;QACF;QACA,MAAMlB,IAAG,GAAI;UACXE,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC;QACD,IAAI,IAAI,CAACC,aAAa,EAAE;UACtBL,IAAI,CAACmB,cAAa,GAAI,IAAI,CAACd,aAAa,EAAG;QAC7C;QAEA,MAAMP,KAAK,CAACW,IAAI,CAAC,8BAA8B,EAAET,IAAI,CAAC;QACtDe,KAAK,CAAC,SAAS,CAAC;QAChB,IAAI,CAACd,cAAa,GAAI,KAAK,EAAG;MAChC,EAAE,OAAOe,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;QACpBD,KAAK,CAAC,SAAS,CAAC;MAClB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}